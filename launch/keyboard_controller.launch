<?xml version="1.0"?>
<launch>
	<node name="keyboard_pub" pkg="keyboard_pub" type="keyboard_pub_node"/>

	<arg name="simulation" default="False"/>
	<arg name="command_topic" default="scaled_pos_traj_controller/command"/>
	<node name="keyboard_controller" pkg="iai_mab_control" type="keyboard_controller.py" output="screen">
		<param name="simulation" value="$(arg simulation)"/>
		<param name="command_topic" value="$(arg command_topic)"/>
	</node>
</launch>
